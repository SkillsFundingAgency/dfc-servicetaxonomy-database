CREATE CONSTRAINT ON (n:Resource) ASSERT n.uri IS UNIQUE;
CALL semantics.addNamespacePrefix("adms","http://www.w3.org/ns/adms#");
CALL semantics.addNamespacePrefix("owl","http://www.w3.org/2002/07/owl#");
CALL semantics.addNamespacePrefix("skosxl", "http://www.w3.org/2008/05/skos-xl#");
CALL semantics.addNamespacePrefix("org","http://www.w3.org/ns/org#");
CALL semantics.addNamespacePrefix("xsd","http://www.w3.org/2001/XMLSchema#");
CALL semantics.addNamespacePrefix("iso-thes", "http://purl.org/iso25964/skos-thes#");
CALL semantics.addNamespacePrefix("skos","http://www.w3.org/2004/02/skos/core#");
CALL semantics.addNamespacePrefix("rdfs","http://www.w3.org/2000/01/rdf-schema#");
CALL semantics.addNamespacePrefix("at","http://publications.europa.eu/ontology/authority/");
CALL semantics.addNamespacePrefix("dct","http://purl.org/dc/terms/");
CALL semantics.addNamespacePrefix("rdf","http://www.w3.org/1999/02/22-rdf-syntax-ns#");
CALL semantics.addNamespacePrefix("esco","http://data.europa.eu/esco/model#");
CALL semantics.addNamespacePrefix("rov","http://www.w3.org/ns/regorg#");
CALL semantics.addNamespacePrefix("dcat","http://www.w3.org/ns/dcat#");
CALL semantics.addNamespacePrefix("euvoc","http://publications.europa.eu/ontology/euvoc#");
CALL semantics.addNamespacePrefix("prov","http://www.w3.org/ns/prov#");
CALL semantics.addNamespacePrefix("foaf","http://xmlns.com/foaf/0.1/");
CALL semantics.addNamespacePrefix("qdr","http://data.europa.eu/esco/qdr#");
CALL semantics.addNamespacePrefix("ncs","http://nationalcareers.service.gov.uk/taxonomy#");