CALL semantics.importRDF("file:///var/lib/neo4j/import/__ESCOFILE__", "Turtle", { handleMultival: 'ARRAY', languageFilter: "en" });
match(s:esco__Skill)WITH REDUCE(c = HEAD(s.skos__altLabel), n IN TAIL(s.skos__altLabel) | c + '|' + n) AS altLabText, s WITH REDUCE(c = HEAD(s.skos__hiddenLabel), n IN TAIL(s.skos__hiddenLabel) | c + '|' + n) AS altHiddenText, s, altLabText set s.skos__compositeAltLabel =toLower(head(s.skos__prefLabel) +case altLabText when null then '' else '|' end + coalesce (altLabText,'') + case altHiddenText when null then '' else '|' end + coalesce (altHiddenText,'') );